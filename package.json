{
  "name": "@mymoid/mymoidapis-node-scaffold",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run typecheck && concurrently -k -p \"[{name}]\" -n \"typecheck,node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "serve": "nodemon --delay 3000ms dist/index.js",
    "lint": "eslint . --ext \".js,.ts,.tsx,.ts.snap\"",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|graphql|md|mdx)\"",
    "typecheck": "tsc",
    "watch-ts": "tsc -w",
    "production-ts": "tsc --sourceMap false",
    "validate": "npm run lint && npm run prettier -- --list-different && npm run typecheck",
    "test": "jest --watch",
    "cover": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run typecheck"
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.13",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^10.12.19",
    "@types/passport": "^1.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.0.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-jest": "^22.2.2",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "jest-environment-node": "^24.0.0",
    "jest-in-case": "^1.0.2",
    "jest-runner-eslint": "^0.7.4",
    "jest-runner-tsc": "^1.3.2",
    "lint-staged": "^8.1.1",
    "mongodb": "^3.1.13",
    "mongodb-memory-server": "^3.0.0",
    "nodemon": "^1.18.9",
    "pluggable-babel-eslint": "^0.3.0",
    "prettier": "^1.16.1",
    "supertest": "^4.0.2",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "aws-xray-sdk": "^2.4.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "cors": "2.8.5",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-async-errors": "3.1.1",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "1.6.6",
    "mongoose": "^5.8.1",
    "passport": "^0.4.1"
  },
  "author": "",
  "license": "ISC"
}
