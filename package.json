{
  "name": "@mymoid/mymoidapis-node-scaffold",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "engines": {
    "node": "15.12.0"
  },
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"typecheck,node\" -c \"yellow.bold,cyan.bold\" \"npm run typecheck\" \"npm run serve\"",
    "serve": "nodemon --delay 3000ms dist/index.js",
    "dev": "ts-node ./src/index",
    "build": "kcd-scripts build --no-ts-defs",
    "debug": "node --inspect-brk dist/index.js --runInBand",
    "lint": "kcd-scripts lint",
    "typecheck": "kcd-scripts typecheck",
    "validate": "npm run test --silent -- --coverage && npm run typecheck --silent",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "test:debug": "node --inspect-brk ./node_modules/kcd-scripts/dist/scripts/test.js --runInBand",
    "cover": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.14",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-decorators": "7.13.5",
    "@babel/preset-env": "7.13.12",
    "@types/cors": "2.8.10",
    "@types/express": "^4.17.0",
    "@types/express-jwt": "6.0.1",
    "@types/faker": "5.1.7",
    "@types/jest": "^23.3.13",
    "@types/json2csv": "5.0.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/loglevel": "1.6.3",
    "@types/node": "14.14.31",
    "concurrently": "^4.1.0",
    "csv-generate": "3.3.0",
    "csv-stringify": "5.6.2",
    "docker-compose": "0.23.6",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "26.6.3",
    "jest-environment-node": "26.6.2",
    "jest-in-case": "^1.0.2",
    "jest-runner-tsc": "1.6.0",
    "kcd-scripts": "^8.1.1",
    "lint-staged": "^8.1.1",
    "msw": "0.27.0",
    "nodemon": "^1.18.9",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "@babel/runtime": "7.13.10",
    "aws-sdk": "2.610.0",
    "aws-xray-sdk": "^2.4.0",
    "axios": "0.19.2",
    "bcrypt": "^5.0.0",
    "cors": "2.8.5",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^6.2.0",
    "elastic-builder": "2.13.0",
    "express": "^4.17.1",
    "express-async-errors": "3.1.1",
    "express-jwt": "^5.3.1",
    "loglevel": "1.6.6",
    "moment": "2.29.1",
    "pg": "8.5.1",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.32"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist",
    "*.ignored.*",
    "*.d.ts"
  ],
  "author": "",
  "license": "ISC"
}
